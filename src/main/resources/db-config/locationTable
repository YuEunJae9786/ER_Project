##orcle
CREATE TABLE KICKLIST 
(
    MARKNO NUMBER(10) PRIMARY KEY, 
    PCODE VARCHAR2(100) NOT NULL, 
    CCODE VARCHAR2(100) NOT NULL, 
    LOCATION_X NUMBER(10) NOT NULL, 
    LOCATION_Y NUMBER(10) NOT NULL, 
    ISRENTAL NUMBER(2),
    rentalDate date
);

##mysql
CREATE TABLE KICKLIST 
(
    MARKNO int(10) PRIMARY KEY, 
    PCODE varchar(100) NOT NULL, 
    CCODE varchar(100) NOT NULL, 
    LOCATION_X varchar(100) NOT NULL, 
    LOCATION_Y varchar(100) NOT NULL, 
    ISRENTAL int(2),
    rentalDate datetime
);
